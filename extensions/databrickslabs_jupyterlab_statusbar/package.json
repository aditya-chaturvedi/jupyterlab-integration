{
  "name": "databrickslabs-jupyterlab-statusbar",
  "version": "1.0.0-rc.3",
  "description": "A JupyterLab extension to show starting status of Databricks clusters.",
  "keywords": [
    "jupyter",
    "jupyterlab",
    "jupyterlab-extension"
  ],
  "license": "SEE LICENSE IN LICENSE",
  "author": "Bernhard Walter",
  "files": [
    "lib/**/*.{d.ts,eot,gif,html,jpg,js,js.map,json,png,svg,woff2,ttf}",
    "style/**/*.{css,eot,gif,html,jpg,json,png,svg,woff2,ttf}"
  ],
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "style": "style/index.css",
  "scripts": {
    "build": "tsc",
    "clean": "rimraf lib && rimraf tsconfig.tsbuildinfo",
    "prepare": "npm run clean && npm run build",
    "watch": "tsc -w"
  },
  "dependencies": {
    "@jupyterlab/application": "~1.0.9",
    "@jupyterlab/apputils": "~1.0.5",
    "@jupyterlab/coreutils": "~3.0.1",
    "@jupyterlab/notebook": "~1.0.5",
    "@jupyterlab/services": "~4.0.5",
    "@jupyterlab/statusbar": "~1.0.5"
  },
  "devDependencies": {
    "rimraf": "^2.6.1",
    "typescript": "~3.5.2"
  },
  "sideEffects": [
    "style/*.css"
  ],
  "jupyterlab": {
    "extension": true
  },
  "directories": {
    "lib": "lib"
  }
}
